  "lint-staged": {
    "{src,apps,libs,test}/**/*.{js,jsx,ts,tsx,css,scss}": [
      "npm run format",
      "npm run lint:fix"
    ]
  },
  "scripts": {
    "postinstall": "husky install",
    "pre-commit": "lint-staged",
    "prebuild": "rimraf dist",
    "typesync": "typesync --ignoredeps=dev",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "build:em-api-layer": "nest build",
    "em-api-layer:dev": "yarn build:em-api-layer && nest start --debug --watch",
    "em-api-layer:prod": "node --optimize-for-size --gc-interval=2 dist/apps/em-api-layer/apps/em-api-layer/src/main",
    "build:auth": "nest build ",
    "auth:dev": "yarn build:auth && nest start auth-service-provider --debug --watch",
    "auth:prod": "node --optimize-for-size --gc-interval=2 dist/apps/auth-service-provider/apps/auth-service-provider/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.{js,jsx,ts,tsx,css,scss}\" --fix",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.{js,jsx,ts,tsx,css,scss}\" --fix --quiet --fix-type layout --format=codeframe",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.{js,jsx,ts,tsx,css,scss}\" --fix-dry-run --fix-type layout --format=codeframe",
    "typeorm-seeder": "node --require ts-node/register ./node_modules/typeorm-seeding/dist/cli.js",
    "seed:run": "yarn typeorm-seeder seed",
    "db:setup": "yarn schema:drop && yarn migration:run && yarn seed:run",
    "deep:check": "node_modules/madge/bin/cli.js --warning --circular --extensions js .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/em-api-layer/test/jest-e2e.json"
  },
